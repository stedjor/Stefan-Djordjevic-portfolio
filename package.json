{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:sr": "ng serve --configuration=sr",
    "build": "ng build",
    "build:sr": "ng build --configuration=sr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/translate",
    "extract-i18n": "ng xi18n portfolio --i18n-format xlf --output-path src/translate --i18n-locale sr && ng run portfolio:xliffmerge",
    "start-en": "ng serve --configuration=en"
